{
  "name": "gulp-wordpress-backup",
  "version": "0.0.2",
  "description": "Painless Wordpress backup with Node.js",
  "main": "gulpfile.js",
  "author": "Peter Bailey (peabay.xyz)",
  "license": "ISC",
  "scripts": {
    "upload": "scp 01-14-17_13.05.28/files.tar.gz peter@peterbailey.eu:sinclair.peterbailey.eu && ssh -t peterbailey.eu 'cd sinclair.peterbailey.eu ; bash'"
  },
   "keywords": [
    "wordpress",
    "gulp",
    "backup",
    "migrate",
    "sql"
  ],
  "gwb": {
    "wppath": "../Wordpress",
    "dbhost": "localhost",
    "dbuser": "wordpress",
    "dbpass": "wordpress",
    "dbdatabase": "wordpress",
    "dbprefix": "wp_",
    "archiver": "tar.gz",
    "replace": {
      "localhost": "sinclair.peabay.xyz"
    },
    "include": [
      "/wp-content"
    ],
    "exclude": [
      "wp-content/themes/**/node_modules",
      "wp-content/themes/**/vendor"
    ],
    "sshUser": "peter",
    "sshHost": "peterbailey.eu",
    "sshPath": "sinclair.peterbailey.eu"
  },
  "gwblive": {
    "wppath": "../Wordpress",
    "dbhost": "localhost",
    "dbuser": "wordpress",
    "dbpass": "wordpress",
    "dbdatabase": "wordpress",
    "dbprefix": "wp_",
    "archiver": "tar.gz",
    "replace": {
      "localhost/ellanse": "ellanse.com",
      "localhost/perfectha": "perfectha.com"
    },
    "include": [
      "/wp-content"
    ],
    "exclude": false,
    "sshUser": "peter",
    "sshHost": "peterbailey.eu",
    "sshPath": "sinclair.peterbailey.eu"
  },
  "devDependencies": {},
  "dependencies": {
    "colors": "^1.1.2",
    "del": "^2.2.2",
    "exec": "^0.2.1",
    "gulp": "^3.9.1",
    "gulp-batch-replace": "0.0.0",
    "gulp-exec": "^2.1.3",
    "gulp-gzip": "^1.4.0",
    "gulp-replace": "^0.5.4",
    "gulp-sequence": "^0.4.6",
    "gulp-string-replace": "^0.3.0",
    "gulp-tar": "^1.9.0",
    "gulp-zip": "^3.2.0",
    "inquirer": "^1.2.3",
    "mkdirp": "^0.5.1",
    "mysqldump": "^1.3.1",
    "node-datetime": "^1.0.0",
    "prepend-file": "^1.3.1",
    "underscore": "^1.8.3",
    "utf8-byte-length": "^1.0.4",
    "yargs": "^6.5.0"
  }
}